<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/mattermind_quiz"
    android:visibility="gone"
    >
    <TextView
        android:id="@+id/view_mattermind_quiz_dates"
        android:layout_marginTop="6dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:textAlignment="center"
        android:text="(E_V_E_N_T_  _S_C_H_E_D_U_L_E)"
        android:textSize="40sp"
        android:textColor="#ffffff"/>
    <TextView
        android:layout_marginTop="10dp"
        android:id="@+id/view_mattermind_quiz_date"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:textColor="#ffffff"
        android:text=""
        android:textSize="20sp"
        android:layout_gravity="center"
        android:textAlignment="center"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/roundrect"
        android:layout_marginTop="16dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="4dp"
            android:background="@drawable/spons_draw"
            >
            <TextView
                android:id="@+id/view_mattermind_quiz_eligibility"
                android:padding="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="eligibility criteria"
                android:layout_centerInParent="true"

                android:textSize="30sp"
                android:textColor="#ffffff"/>
        </RelativeLayout>
        <TextView
            android:layout_marginTop="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/mattermind_quiz_eligibility"
            android:textSize="15sp"
            android:padding="8dp"

            android:layout_gravity="center"
            android:textColor="#ffffff"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/roundrect"
        android:layout_marginTop="16dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="4dp"
            android:background="@drawable/spons_draw"
            >
            <TextView
                android:id="@+id/view_mattermind_quiz_formation_teams"
                android:padding="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="formation of teams"
                android:layout_centerInParent="true"

                android:textSize="30sp"
                android:textColor="#ffffff"/>
        </RelativeLayout>
        <TextView
            android:layout_marginTop="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/mattermind_quiz_formation_teams"
            android:textSize="15sp"
            android:padding="8dp"

            android:layout_gravity="center"
            android:textColor="#ffffff"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/roundrect"
        android:layout_marginTop="16dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="4dp"
            android:background="@drawable/spons_draw"
            >
            <TextView
                android:id="@+id/view_mattermind_quiz_general_rules"
                android:padding="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="general rules"
                android:layout_centerInParent="true"

                android:textSize="30sp"
                android:textColor="#ffffff"/>
        </RelativeLayout>
        <TextView
            android:layout_marginTop="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/mattermind_quiz_general_rules"
            android:textSize="15sp"
            android:padding="8dp"

            android:layout_gravity="center"
            android:textColor="#ffffff"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/roundrect"
        android:layout_marginTop="16dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="4dp"
            android:background="@drawable/spons_draw"
            >
            <TextView
                android:id="@+id/view_mattermind_quiz_requirements"
                android:padding="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="requirements"
                android:layout_centerInParent="true"

                android:textSize="30sp"
                android:textColor="#ffffff"/>
        </RelativeLayout>
        <TextView
            android:layout_marginTop="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/mattermind_quiz_requirements"
            android:textSize="15sp"
            android:padding="8dp"

            android:layout_gravity="center"
            android:textColor="#ffffff"/>
    </LinearLayout>


    <LinearLayout
        android:layout_margin="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="center"
        android:background="@drawable/roundrect"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@drawable/spons_draw">
            <TextView
                android:id="@+id/view_mattermind_quiz_reg_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_centerInParent="true"

                android:text="REGISTRATION FEE\n     ( click to register )"
                android:textSize="20sp"
                android:textColor="#ffffff"
                />
        </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/mattermind_quiz_register_single"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="8dp"
                >
                <com.jackandphantom.circularprogressbar.CircleProgressbar
                    android:id="@+id/single"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_centerInParent="true"
                    android:background="@drawable/circle"
                    app:cpb_foregroundProgressColor="@color/colorAccent"
                    app:cpb_foregroundProgressWidth="15"
                    app:cpb_progress="100"
                    app:cpb_roundedCorner="true"
                    app:cpb_touchEnabled="false"
                    android:padding="10dp"
                    />
                <TextView
                    android:id="@+id/mattermind_quiz_reg_single"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginTop="8dp"
                    android:textSize="20sp"
                    android:text="rs 200\nper member"
                    android:layout_centerInParent="true"
                    android:textColor="#ffffff"
                    />

            </RelativeLayout>
            <TextView
                android:id="@+id/single_text_mattermind_quiz"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="single"
                android:textColor="#ffffff"
                android:padding="3dp"
                android:textAlignment="center"
                android:textSize="18sp"

                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/mattermind_quiz_register_team"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="8dp"
                >
                <com.jackandphantom.circularprogressbar.CircleProgressbar
                    android:id="@+id/team"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_centerInParent="true"
                    android:background="@drawable/circle"
                    app:cpb_foregroundProgressColor="@color/colorAccent"
                    app:cpb_foregroundProgressWidth="15"
                    app:cpb_progress="100"
                    app:cpb_roundedCorner="true"
                    app:cpb_touchEnabled="false"
                    android:padding="10dp"
                    />
                <TextView
                    android:id="@+id/mattermind_quiz_reg_team"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginTop="3dp"
                    android:textSize="15sp"
                    android:text="rs 400\nper team"
                    android:layout_centerInParent="true"
                    android:textColor="#ffffff"
                    />

            </RelativeLayout>
            <TextView
                android:id="@+id/team_text_mattermind_quiz"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="team"
                android:textColor="#ffffff"
                android:padding="3dp"
                android:textAlignment="center"
                android:textSize="18sp"

                />
        </LinearLayout>

    </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@drawable/roundrect"
        android:layout_marginTop="16dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="4dp"
            android:background="@drawable/spons_draw"
            >
            <TextView
                android:id="@+id/view_mattermind_quiz_coordinators"
                android:padding="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="event coordinator"
                android:layout_centerInParent="true"

                android:textSize="30sp"
                android:textColor="#ffffff"/>
        </RelativeLayout>
        <TextView
            android:id="@+id/view_mattermind_quiz_coordinator_text"
            android:layout_marginTop="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="NANCY AGARWAL : +919435993800"
            android:textSize="15sp"
            android:padding="8dp"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:textColor="#ffffff"/>
    </LinearLayout>
</LinearLayout>